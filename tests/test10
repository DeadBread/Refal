c : boolean;
procedure numb1(y, x : integer;i : integer);
var d : boolean;
begin 
i := i+1;
d := 7;
if (d) then 
if x<y then i := i-1 
else writeln(d) 
else writeln(i);
end;

function num2(k : integer) : integer;
begin 
numb1(k, k, k);
repeat 
k := k+1;
k := k+1;
k := k+1;
until true;
end;

begin 
a := 12;
b := 13;
